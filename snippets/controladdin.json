{
    "Snippet: Control Add-In": {
        "prefix": "tcontroladdin",
        "body": [
            "controladdin ${1:MyControlAddIn}",
            "{",
            "\tRequestedHeight = ${2:300};",
            "\tMinimumHeight = ${3:300};",
            "\tMaximumHeight = ${4:300};",
            "\tRequestedWidth = ${5:700};",
            "\tMinimumWidth = ${6:700};",
            "\tMaximumWidth = ${7:700};",
            "\tVerticalStretch = ${8|true,false|};",
            "\tVerticalShrink = ${9|true,false|};",
            "\tHorizontalStretch = ${10|true,false|};",
            "\tHorizontalShrink = ${11|true,false|};",
            "\tScripts = ",
            "\t\t'${12:script1.js}',",
            "\t\t'${13:script2.js}';",
            "\tStyleSheets =",
            "\t\t'${14:style.css}';",
            "\tStartupScript = '${15:startupScript.js}';",
            "\tRecreateScript = '${16:recreateScript.js}';",
            "\tRefreshScript = '${17:refreshScript.js}';",
            "\t",
            "\tevent ${18:MyEvent}(${19})",
            "\t",
            "\tprocedure ${20:MyProcedure}($0)",
            "}"
        ],
        "description": "Snippet: Control Add-In"
    }
}